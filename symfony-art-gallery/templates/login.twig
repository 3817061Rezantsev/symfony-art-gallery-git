
<html>
<head>
    <meta charset="UTF-8">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>

<h1>Authentication</h1>

<div class="col-3">
    {{ form(form_login) }}
</div>


{% if err  %}
    <div>
        {{ err.messageKey|trans(err.messageData, 'security') }}
    </div>
{% endif %}
{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>
